    "build-sandbox": "lerna run --scope sandbox build",
    "prettify": "prettier --write",
    "lint": "eslint . --fix",
    "lint:staged": "eslint --fix"
  },
  "repository": "https://github.com/ameerthehacker/hackbox.git",
  "author": "ameerthehacker",
  "license": "apache",
  "private": true,
  "dependencies": {
    "lerna": "^3.19.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  }
}
