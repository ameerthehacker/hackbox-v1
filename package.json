{
  "name": "hackbox",
  "version": "0.0.1",
  "description": "Super powered dev environment setup using docker",
  "main": "main.js",
  "scripts": {
    "dev-client": "cross-env BROWSER=none react-scripts start",
    "build-client": "react-scripts build",
    "start": "electron .",
    "dev-electron": "cross-env DEBUG=true NODE_ENV=development electron .",
    "dev": "concurrently --kill-others yarn:dev-client yarn:dev-electron"
  },
  "repository": "https://github.com/ameerthehacker/hackbox",
  "author": "ameerthehacker",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.5.1",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "dockerode": "^3.0.2",
    "electron": "^7.1.7",
    "emotion-theming": "^10.0.19",
    "install": "^0.13.0",
    "npm": "^6.13.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "node-fetch": "^2.6.0",
    "react-scripts": "^3.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
